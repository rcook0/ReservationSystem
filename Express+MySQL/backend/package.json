    "db:generate:prisma": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.0",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "prisma": "^5.18.0",
    "sequelize-cli": "^6.6.2"
  }
}
