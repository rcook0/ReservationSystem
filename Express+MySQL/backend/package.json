{
  "name": "airline-reservation-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "db:migrate:sequelize": "npx sequelize-cli db:migrate",
    "db:seed:sequelize": "npx sequelize-cli db:seed:all",
